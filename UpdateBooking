#UpdateBooking

PUT =  https://restful-booker.herokuapp.com/booking/:id

Headers :

Key = Content-Type
Value = application/json
Key = Accept
Value = application/json
Key = Cookie
Valu = token=tokendegeri


Body :

* raw se√ßilmeli
{
